<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="extend_payment_process_page" inherit_id="payment.payment_process_page" name="Payment Processing Page">
        <xpath expr="//div[hasclass('o_website_payment')]" position="replace">
           <div class="container o_website_payment mt-3">
               <div class="o_payment_processing" style="background: #eee;border-radius: 2px; border-bottom: 3px solid #1ea6a6;">
                   <div class="row px-3">
                       <div class="o_payment_processing_content w-100 p-2">
                            <!-- here should be the content generated by payment_processing.js -->
                       </div>
                   </div>
               </div>
           </div>
        </xpath>
    </template>
</odoo>